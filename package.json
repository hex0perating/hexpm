{
  "name": "hexpm",
  "version": "1.0.0",
  "description": "Hex0s Package Manager, written in Deno",
  "main": "index.js",
  "scripts": {
    "install-deno": "echo Warning: This is only for Linux users! && curl -fsSL https://deno.land/install.sh | sh && echo ",
    "compile": "~/.deno/bin/deno compile --unstable --allow-net --allow-run --allow-read --allow-write index.js",
    "start": "~/.deno/bin/deno run --unstable --allow-net --allow-run --allow-read --allow-write index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hex0perating/hexpm.git"
  },
  "keywords": [],
